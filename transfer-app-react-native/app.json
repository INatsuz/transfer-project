{
  "expo": {
    "name": "All Aboard Chauffeurs",
    "slug": "allaboardchauffeurs",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/notification-icon.png"
    },
    "scheme": "vrtransfer",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.inatsuz.allaboardchauffeurs"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1D1D1D"
      },
      "package": "com.inatsuz.allaboardchauffeurs"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f0e82b79-24f7-4263-951c-e491b084c20e"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
